@model IEnumerable<IEnumerable<WebApplication1.Models.Predicate>>
@using WebApplication1.Models
<div id="results">
    <h4>@ViewBag.Status</h4><br />
    <h4>Список предикатов, вызывающих противоречие</h4>
    <ol>
        @for (int j = 0; j < Model.First<IEnumerable<WebApplication1.Models.Predicate>>().Count<Predicate>(); j++)
        {
        <li>
            @for (int i = 0; i < Model.Count<IEnumerable<WebApplication1.Models.Predicate>>(); i++)
            {
                var item = Model.ElementAt(i).ElementAt(j);
                var delim = i < Model.Count<IEnumerable<WebApplication1.Models.Predicate>>() - 1 ? " | " : "";
                @item.OpA.GetNameToDisplay()@Html.DisplayFor(modelItem => item.OperandA) @item.Op.GetNameToDisplay() @item.OpB.GetNameToDisplay()@Html.DisplayFor(modelItem => item.OperandB)@delim
            }
        </li>
        }
    </ol>
</div>

